        <PackageReference Include="Krafs.Rimworld.Ref" Version="1.6.4633" ExcludeAssets="runtime"/>
        <PackageReference Include="Lib.Harmony" Version="2.4.2" ExcludeAssets="runtime"/>
    </ItemGroup>
    
    <ItemGroup>
      <Reference Include="AlienRace">
        <HintPath>..\..\..\AlienRaces\Assemblies\AlienRace.dll</HintPath>
      </Reference>
    </ItemGroup>
</Project>